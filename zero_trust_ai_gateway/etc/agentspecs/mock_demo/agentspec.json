{
  "display_name": "Mock AI Agent for Demo",
  "model_type": "mock",
  "model_name": "mock-model",
  "metadata": {
    "agent_proxy": {
      "class_name": "zero_trust_ai_gateway.services.agentproxies.agentproxy.MockAgentProxy",
      "config": {
        "mock_latency_ms": 50.0,
        "request_timeout": 10.0,
        "max_retries": 2,
        "content_filtering": false,
        "prompt_injection_detection": true
      }
    },
    "security_policy": {
      "authorized_users": ["demo_user", "test_user", "admin"],
      "unauthorized_users": [],
      "required_capabilities": ["text_generation"],
      "rate_limit_per_user": 1000,
      "max_context_length": 4000,
      "sensitive_data_detection": false,
      "prompt_injection_protection": true
    },
    "compliance": {
      "audit_logging": false,
      "pii_detection": false,
      "data_retention_days": 1,
      "encryption_at_rest": false,
      "gdpr_compliant": false,
      "hipaa_compliant": false
    },
    "performance": {
      "target_latency_ms": 50,
      "cache_enabled": true,
      "batch_processing": false,
      "connection_pooling": false,
      "memory_optimization": false
    },
    "cost_optimization": {
      "token_usage_monitoring": false,
      "cost_limits_per_user": 0.0,
      "automatic_model_selection": false
    }
  }
}